<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PrimeBlue">#304FFE</Color>
    <SolidColorBrush x:Key="PrimeBlueBrush" Color="{StaticResource PrimeBlue}"/>

    <Color x:Key="MousseOverBlue">#0026CA</Color>
    <SolidColorBrush x:Key="MousseOverBlueBrush" Color="{StaticResource MousseOverBlue}"/>

    <Color x:Key="PressedBlue">#7A7CFF</Color>
    <SolidColorBrush x:Key="PressedBlueBrush" Color="{StaticResource PressedBlue}"/>



</ResourceDictionary>